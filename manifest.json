{
    "manifest_version": 2,

    "name": "Bucknell Course Selection Sidebar",
    "description": "Adds features to the Bucknell Course Selection Page. Developed by Patrick Bacon-Blaber, Eileen Cook, Ethan Vynalek, Alex Wade",
    "version": "1.0.2",

    "author": "Team SideBar",

    "icons": {
        "16": "images/icon16.png",
        "48": "images/icon48.png",
        "128": "images/icon128.png"
    },

    "browser_action": {
        "default_icon": {
            "16": "images/icon16.png",
            "48": "images/icon48.png",
            "128": "images/icon128.png"
        },
        "default_popup": "popup/popup.html"
    },

    "permissions": [
        "storage"
    ],

    "web_accessible_resources": [
        "search-logic/courseList.JSON",
        "images/icon.png"
    ],

    "content_scripts": [
        {
            "matches": ["https://www.bannerssb.bucknell.edu/ERPPRD/*SchedDisplay"],
            "css": ["style.css"],
            "js": ["jquery/jquery.min.js", "sidebar.js", "jquery/jquery-ui.min.js"]
        }
    ],

    "key": ""
}
